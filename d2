//perfect squares without using squares
-----------------------------------------
	#include <stdio.h>
	int main() {
	    // Write C code here
	    int i,a;
	    for(i=1;i<100;i++)
	    {
	       a= i*i;
	       if(a>40 && a<100)
	       printf("%d ",a);
	    }

	    return 0;
	}
